<BFTypedBinder binderType="enrichment" abstract="true">
   <types>
      <entity>
         <enrichment/>
      </entity>
   </types>
   <shared>
      <ZTPlacementData randomRotate="false" gridSnap="true" autoFootprint="true">
         <stompData>
            <delete>
               <Poop/>
            </delete>
            <allow/>
         </stompData>
      </ZTPlacementData>
      <BFGCollisionData isTraversable="true"/>
      <BFAIEntityDataShared b_SendSlap="true"/>
   </shared>
   <binder>
      <BFNamedBinder binderName="mainObj">
         <instance>
            <BFPhysObj>
               <BFCollisionComponent/>
               <BFDistanceCullComponent firstPersonOnly="true" cullDistance="65"/>
            </BFPhysObj>
         </instance>
      </BFNamedBinder>
      <BFBinder required="1">
         <instance>
            <ZTPhotoAnalysisComponent/>
         </instance>
      </BFBinder>
   </binder>
</BFTypedBinder>
