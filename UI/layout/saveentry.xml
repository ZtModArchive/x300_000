<UIToggleButton name="LoadEntry">
	<UIRegion x="10" w="177" h="16" />
	<UIAspect alwayshit="true">
		<standard>
			<normal>
				<BFFont>
					<BFColor r="59" g="83" b="66" a="255" />
				</BFFont>
			</normal>
			<highlighted>
				<BFFont>
					<BFColor r="255" g="255" b="201" a="255" />
				</BFFont>
			</highlighted>
			<activated>
				<BFFont>
					<BFColor r="255" g="255" b="0" a="255" />
				</BFFont>
			</activated>
		</standard>
		<alternate>
			<normal>
				<BFFont>
					<BFColor r="255" g="255" b="255" a="255" />
				</BFFont>
			</normal>
			<highlighted>
				<BFFont>
					<BFColor r="255" g="255" b="255" a="255" />
				</BFFont>
			</highlighted>
			<activated>
				<BFFont>
					<BFColor r="255" g="255" b="0" a="255" />
				</BFFont>
			</activated>
		</alternate>
	</UIAspect>
	<text format="savegame:filename" />
	<doubleclick>
		<event msg="UI_CHILD" data="UIChildData" name="Save Game Button">
			<child msg="UI_ACTIVATE" />
		</event>
	</doubleclick>
</UIToggleButton>
