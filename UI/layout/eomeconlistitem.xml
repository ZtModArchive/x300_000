<UILayout name="eomeconlistitem">
	<UIRegion x="0" y="0" w="300" h="24" xalign="min" yalign="min" walign="len" halign="len"/>
	<children>
		<UIText name="label">
			<UIRegion x="0" y="0" w="200" h="24" xalign="min" yalign="min" walign="len" halign="len"/>
			<UIAspect>
				<default>
					<BFFont align="right">
						<BFColor r="255" g="255" b="255" a="255"/>
					</BFFont>
				</default>
			</UIAspect>
		</UIText>
		<UIText name="value">
			<UIRegion x="200" y="0" w="60" h="24" xalign="min" yalign="min" walign="len" halign="len"/>
			<UIAspect>
				<default>
					<BFFont align="right">
						<BFColor r="0" g="255" b="0" a="255"/>
					</BFFont>
				</default>
			</UIAspect>
		</UIText>
	</children>
</UILayout>
