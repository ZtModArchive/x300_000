<UILayout name="bar item">
   <UIRegion x="0" y="0" w="50" h="300" xalign="min" yalign="min" walign="len" halign="len"/>
   <UIAspect>
      <default/>
   </UIAspect>
   <children>
      <UIListBox name="bar box" autosize="false" columns="1" rows="-1">
         <UIRegion x="0" y="0" w="50" h="300" xalign="min" yalign="min" walign="len" halign="max"/>
         <UIState active="normal" visible="normal"/>
         <UIAspect>
            <default image=""/>
         </UIAspect>
         <children>
            <UIListBox name="bar section" autosize="false" columns="0" rows="0">
               <UIRegion x="15" y="0" w="20" h="250" xalign="min" yalign="min" walign="len" halign="len"/>
               <UIState active="normal" visible="normal"/>
               <UIAspect>
                  <default image=""/>
               </UIAspect>
               <children>
                  <UILayout name="bar">
                     <UIRegion x="0" y="0" w="20" h="100" xalign="min" yalign="% min" walign="len" halign="% min"/>
                     <UIAspect>
                        <default image="ui/shared/white1x1.dds">
                           <BFColor r="180" g="180" b="180" a="255"/>
                        </default>
                     </UIAspect>
                  </UILayout>
               </children>
            </UIListBox>
            <UILayout name="gap">
               <UIRegion x="0" y="0" w="0" h="4" xalign="min" yalign="min" walign="len" halign="len"/>
               <UIAspect>
                  <default/>
               </UIAspect>
            </UILayout>
            <UIText name="label">
               <UIRegion x="0" y="0" w="50" h="24" xalign="min" yalign="min" walign="len" halign="len"/>
               <UIAspect>
                  <default>
                     <BFFont align="center">
                        <BFColor r="120" g="120" b="120"/>
                     </BFFont>
                     <BFColor r="255" g="255" b="255" a="255"/>
                  </default>
               </UIAspect>
               <text format="zoostatus:bar_item_format"/>
            </UIText>
         </children>
      </UIListBox>
   </children>
</UILayout>
