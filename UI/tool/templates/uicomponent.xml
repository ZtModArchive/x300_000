<UIListBox name="editUIComponent" rows="-1" autosize="true">
	<UIRegion walign="max" />
	<children>
		<UIStatic>
			<UIRegion h="20" walign="max" />
			<UIAspect string="UIComponent Info" />
		</UIStatic>
		<UILayout>
			<UIRegion x="20" h="20" walign="max" />
			<children>
				<UIStatic>
					<UIRegion x="0" y="0" w="50" h="20" walign="% min"/>
					<UIAspect string="name" />
				</UIStatic>
				<UIText name="name">
					<UIRegion x="50" y="0" w="100" h="20" xalign="% min" walign="% min" />
				</UIText>
			</children>
		</UILayout>
	</children>
</UIListBox>
